<Window x:Class="Pilas_Colas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow">
    <Grid>
        <Label Content="Valor:" HorizontalAlignment="Left" Margin="62,49,0,0" VerticalAlignment="Top" Height="29" Width="88"/>
        <TextBox x:Name="txtValor" HorizontalAlignment="Left" Height="23" Margin="150,53,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btnPush" Content="PUSH" HorizontalAlignment="Left" Margin="75,106,0,0" VerticalAlignment="Top" Width="141" Height="57" Click="btnPush_Click"/>
        <Button x:Name="btnPop" Content="POP" HorizontalAlignment="Left" Margin="75,189,0,0" VerticalAlignment="Top" Width="141" Height="57" Click="btnPop_Click"/>
        <TextBox x:Name="txtMostrar" HorizontalAlignment="Left" Height="218" Margin="308,123,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="351"/>
        <Button x:Name="btnPushCola" Content="Push Cola" HorizontalAlignment="Left" Margin="75,274,0,0" VerticalAlignment="Top" Width="141" Height="67" Click="btnPushCola_Click"/>
        <Button x:Name="btnPopCola" Content="Pop Cola" HorizontalAlignment="Left" Margin="75,377,0,0" VerticalAlignment="Top" Width="141" Height="71" Click="btnPopCola_Click"/>

    </Grid>
</Window>
