            <ComboBoxItem Content="A"/>
            <ComboBoxItem Content="B"/>
            <ComboBoxItem Content="C"/>
            <ComboBoxItem Content="D"/>
            <ComboBoxItem Content="E"/>
        </ComboBox>
        <Button x:Name="btnmostrar" Content="Mostrar" HorizontalAlignment="Left" Margin="335,186,0,0" VerticalAlignment="Top" Width="190"/>
        <TextBox x:Name="txtmostrar" HorizontalAlignment="Left" Height="23" Margin="335,142,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="190"/>

