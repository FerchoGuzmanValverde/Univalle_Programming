        <Button x:Name="btnregistrar" Content="Registrar" HorizontalAlignment="Left" Margin="46,109,0,0" VerticalAlignment="Top" Width="75" Click="btnregistrar_Click"/>
        <TextBox x:Name="txtresult" HorizontalAlignment="Left" Height="133" Margin="10,147,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="199"/>

    </Grid>
</Window>
