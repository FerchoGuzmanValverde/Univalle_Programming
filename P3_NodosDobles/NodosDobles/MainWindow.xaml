        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NodosDobles"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="btnAgregar" Content="Agregar" HorizontalAlignment="Left" Margin="21,57,0,0" VerticalAlignment="Top" Width="145" Height="59" Click="btnAgregar_Click"/>
        <TextBox x:Name="txtvalor" HorizontalAlignment="Left" Height="23" Margin="52,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtmostrar" HorizontalAlignment="Left" Height="134" Margin="219,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="226"/>
        <Button x:Name="btnEliminar" Content="Eliminar" HorizontalAlignment="Left" Margin="21,142,0,0" VerticalAlignment="Top" Width="145" Height="69" Click="btnEliminar_Click"/>

    </Grid>
</Window>
