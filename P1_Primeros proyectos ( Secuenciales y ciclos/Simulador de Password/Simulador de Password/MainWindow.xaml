<Window x:Class="Simulador_de_Password.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Simulador_de_Password"
        mc:Ignorable="d"
        Title="MainWindow" Height="290" Width="345">
    <Grid Margin="0,0,2,2">
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="1"/>
                <GradientStop Color="#FF4FCD15" Offset="0.407"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Label Content="Password" HorizontalAlignment="Left" Margin="39,77,0,0" VerticalAlignment="Top" Width="86"/>
        <Label Content="ADMINISTRADOR" HorizontalAlignment="Left" Margin="90,28,0,0" VerticalAlignment="Top" Width="135"/>
        <Button x:Name="btningresar" Content="Ingresar" HorizontalAlignment="Left" Margin="87,125,0,0" VerticalAlignment="Top" Width="126" Click="btningresar_Click"/>
        <Label x:Name="lblacceso" Content="" HorizontalAlignment="Left" Margin="59,182,0,0" VerticalAlignment="Top" Width="166"/>
        <PasswordBox x:Name="txtpassword" HorizontalAlignment="Left" Margin="130,77,0,0" VerticalAlignment="Top" Width="125"/>

    </Grid>
</Window>
