<Window x:Class="Encuentra_el_numero.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Encuentra_el_numero"
        mc:Ignorable="d"
        Title="MainWindow" Height="191" Width="365">
    <Grid>
        <Label x:Name="lblnumero" Content="Ingresa un numero" HorizontalAlignment="Left" Margin="31,38,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtnumero" HorizontalAlignment="Left" Height="23" Margin="183,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblintentos" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top">
            <Label x:Name="lblintentos1" Content=""/>
        </Label>
        <Label x:Name="lblrespuesta" Content="-----------------------" HorizontalAlignment="Left" Margin="48,81,0,0" VerticalAlignment="Top" Width="150"/>
        <Button x:Name="btnverificar" Content="Verificar" HorizontalAlignment="Left" Margin="213,81,0,0" VerticalAlignment="Top" Width="75" Click="btnverificar_Click"/>
        <Label x:Name="lblaviso" Content="" HorizontalAlignment="Left" Margin="139,10,0,0" VerticalAlignment="Top" Width="191"/>

    </Grid>
</Window>
